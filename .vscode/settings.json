{
    "python.pythonPath": ".venv/bin/python",
    "cSpell.words": [
        "Datasheet",
        "HCHO",
        "Molex",
        "PicoBlade",
        "asdict",
        "dataclasses",
        "extensions",
        "getenv",
        "hexdump",
        "influxdb",
        "mosquitto",
        "mypy",
        "paho",
        "plantower",
        "pyserial",
        "pytest",
        "rfind",
        "rhum",
        "strftime",
        "subcommand",
        "subcommands",
        "uart"
    ],
    "cSpell.ignoreWords": [
        "aqmd",
        "avaldebe",
        "gmail",
        "hpma",
        "lcsc",
        "mqttpassword",
        "mqttuser",
        "pmsa",
        "senserion"
    ],
    "editor.formatOnSave": true,
    "python.formatting.provider": "black",
    "python.testing.pytestEnabled": true,
    "python.linting.mypyEnabled": true,
    "python.formatting.blackArgs": [
        "--line-length",
        "100",
        "--target-version",
        "py36"
    ]
}